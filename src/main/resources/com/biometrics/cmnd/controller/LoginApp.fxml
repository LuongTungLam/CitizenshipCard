<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="loginPane" prefHeight="400.0" prefWidth="418.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.biometrics.cmnd.controller.LoginApp">
    <children>
        <Pane layoutX="21.0" layoutY="30.0" prefHeight="354.0" prefWidth="372.0">
            <children>
                <StackPane fx:id="viewFinger" layoutX="86.0" layoutY="65.0" prefHeight="192.0" prefWidth="200.0"
                           style="-fx-border-color: gray;"/>
                <Button fx:id="scan" layoutX="245.0" layoutY="264.0" mnemonicParsing="false" onAction="#scan"
                        text="Scan"/>
                <Button fx:id="login" layoutX="186.0" layoutY="264.0" mnemonicParsing="false" onAction="#login"
                        text="Login"/>
                <ComboBox fx:id="listLiveScan" layoutX="86.0" layoutY="264.0" prefHeight="25.0" prefWidth="84.0"/>
                <Button fx:id="face" layoutX="245.0" layoutY="298.0" mnemonicParsing="false" onAction="#face"
                        text="Face"/>
                <ComboBox fx:id="listCamera" layoutX="86.0" layoutY="298.0" prefHeight="25.0" prefWidth="146.0"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
