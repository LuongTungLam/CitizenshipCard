<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="allSubjectPane" prefHeight="714.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.biometrics.cmnd.controller.AllSubject">
    <TableView fx:id="tableSubject" layoutX="10.0" layoutY="45.0" prefHeight="661.0" prefWidth="719.0">
    </TableView>
    <AnchorPane layoutX="10.0" layoutY="5.0" prefHeight="34.0" prefWidth="719.0" style="-fx-border-color: gray;">
        <children>
            <Button fx:id="add" layoutX="6.0" layoutY="3.0" mnemonicParsing="false" onAction="#add" prefHeight="32.0"
                    prefWidth="43.0">
                <graphic>
                    <ImageView fx:id="iconView" fitHeight="24.0" fitWidth="32.0" pickOnBounds="true"
                               preserveRatio="true"/>
                </graphic>
            </Button>
            <Button fx:id="refresh" layoutX="65.0" layoutY="3.0" mnemonicParsing="false" onAction="#refreshTableView"
                    prefHeight="32.0" prefWidth="43.0">
                <graphic>
                    <ImageView fx:id="iconRefresh" fitHeight="24.0" fitWidth="32.0" pickOnBounds="true"
                               preserveRatio="true"/>
                </graphic>
            </Button>
            <Button fx:id="search" layoutX="124.0" layoutY="3.0" mnemonicParsing="false" onAction="#search"
                    prefHeight="32.0" prefWidth="43.0">
                <graphic>
                    <ImageView fx:id="iconSearch" fitHeight="24.0" fitWidth="32.0" pickOnBounds="true"
                               preserveRatio="true"/>
                </graphic>
            </Button>
            <Button fx:id="logout" layoutX="674.0" layoutY="6.0" mnemonicParsing="false" onAction="#logout"
                    prefHeight="26.0" prefWidth="36.0">
                <graphic>
                    <ImageView fx:id="iconLogout" fitHeight="19.0" fitWidth="25.0" pickOnBounds="true"
                               preserveRatio="true"/>
                </graphic>
            </Button>
            <AnchorPane fx:id="userPane" layoutX="499.0" layoutY="3.0" prefHeight="32.0" prefWidth="166.0"
                        style="-fx-border-color: gray;">
                <children>
                    <ImageView fx:id="imageUser" fitHeight="26.0" fitWidth="36.0" layoutX="14.0" layoutY="3.0"
                               pickOnBounds="true" preserveRatio="true"/>
                    <Label fx:id="nameUser" alignment="CENTER_RIGHT" contentDisplay="RIGHT" disable="true"
                           layoutX="50.0" layoutY="6.0" prefHeight="18.0" prefWidth="101.0"/>
                </children>
            </AnchorPane>
        </children>
    </AnchorPane>
</AnchorPane>
